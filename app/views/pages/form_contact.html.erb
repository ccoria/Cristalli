<div id="contact" class="content">
    <h1><img src="images/titles/contato.png" width="139" height="17" alt="Contato" title="Contato"></h1>
    <div class="box transparent">
        
        <form method="POST" action="send_mail">
            
            <div id="name_field">
                <label for="name">Name<label><input type="text" name="name" />
            </div>
            <div id="email_field">
                <label for="email">Email<label><input type="text" name="email" />
            </div>
            <div id="fone_field">
                <label for="phone">Fone<label><input type="text" name="phone" />
            </div>
            <div id="message_field">
                <label for="message">Message<label><textarea type="text" name="message"></textarea>
            </div>
            
            <input type="submit" value="Enviar" />
            
        </form>
        
    </div>
</div>